import numpy as np
import csv
import scipy.cluster.vq as v1


vector=np.loadtxt(open("Book1.csv", "rb"), delimiter=",", skiprows=1)
vector=v1.whiten(vector)
#result=v1.kmeans(vector,7)
result2 , label=v1.kmeans2(vector,4)
#label2=label[2:1923]
#label1=label[1924:3972]



